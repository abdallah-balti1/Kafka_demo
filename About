        const response = await axios.get(`/manage/pocs/user/${data.user.userId}`);

        const payload = {
            user: {
                userId: data.user.userId,
                firstName: data.user.firstName,
                lastName: data.user.lastName,
                mail: data.user.mail,
                role: data.user.role,
                active: data.user.active
            },
            poc: {
                id: data.poc.id,
                name: data.poc.name,
                startDate: data.poc.startDate,
                endDate: data.poc.endDate,
                url: data.poc.url
            },
            startUsing: data.startUsing,
            endUsing: data.endUsing
        };
        // ... rest of the function
    }
    // ... error handling
};





