export const assignUserToPoc = async (
  user: User,
  poc: Pocs,
  startUsing: string,
  endUsing: string
): Promise<void> => {
  const payload = {
    user,
    poc,
    startusing: startUsing,
    endusing: endUsing,
  };

  console.log("Sending payload to /manage/user/poc:", payload);

  await axios.post("/manage/user/poc", payload);
};
