import axios from "@/utils/customizedAxios";
import { User } from "@/schemas/user";
import { Pocs } from "@/schemas/pocs";

export const assignUserToPoc = async (
  user: User,
  poc: Pocs,
  startUsing: string,
  endUsing: string
): Promise<void> => {
  await axios.post("/v.0.0.1/manage/user/poc", {
    user,
    poc,
    startUsing,
    endUsing,
  });
};
