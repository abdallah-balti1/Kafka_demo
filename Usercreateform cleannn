import axios from "@/utils/customizedAxios";

export const assignUserToPoc = async ({
  userId,
  pocUid,
}: {
  userId: string;
  pocUid: string;
}) => {
  return axios.post("/v.0.0.1/manage/user/poc", {
    user_id: userId,
    poc_id: pocUid,
    start_use: new Date().toISOString().split("T")[0],
  });
};
